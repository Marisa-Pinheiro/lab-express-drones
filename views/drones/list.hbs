<h2>Available drones</h2>
{{#each drones}}
<div>
    <h4>Name: {{name}}</h4>
    <p>Number of propellers: {{propellers}}</p>
    <p>Maximum speed: {{maxSpeed}}</p>
    <a href='/drones/{{_id}}/edit'>Edit</a>
     <form action="/drones/{{_id}}/delete" method="post">
      <button>Delete Drone</button>
      </form>
</div>
{{/each}}